class Solution {
public:
    vector<string> fizzBuzz(int n)
    {
       vector<string> vect;
       string a;
       for(int i=0;i<n;i++)
       {
           a=to_string(i+1);
           if((i+1)%3==0 && (i+1)%5==0)
                vect.push_back("FizzBuzz");
           else if((i+1)%3==0 )
                vect.push_back("Fizz");
           else if((i+1)%5==0)
                vect.push_back("Buzz");
           else
               vect.push_back(a);
       }
       return vect;
    }
};
